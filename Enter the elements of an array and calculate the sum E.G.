//This code finds the sum of the elements in an array 
//and calculates the sum 

#include <stdio.h>

int arraySum(int a[], int n) 
{
   int sum = 0, i;
  //find the sum of elements in the array
   for(i=0; i<n; i++)
   sum = sum+a[i];
   printf("Sum of the array elements is :%d\t", sum);
}
int main()
{
    int a[10], i, n;
    printf("Enter array size: \n");
    scanf("%d", &n);
    printf("Enter %d elements : \n",n);
    for(i=0; i<n;i++)
    scanf("%d", &a[i]);
    arraySum(a,n);
    
}
