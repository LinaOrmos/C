#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
	
    int n;
    scanf("%d", &n);
    //Complete the code to calculate the sum of the five digits on n.
    int res = 0;
    for(int k = 1;k<=10000;k*=10){
        res += (n/k)%10;
    }
    printf("%d",res);
    return 0;
}
