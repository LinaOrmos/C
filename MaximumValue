//This is an examplee for calculating 
//the maximum value with the if-else statement

#include <stdio.h>

int main()
{
  int x, y, z, max;
  printf("enter any three values: \n");
  scanf("%d%d%d",&x,&y,&z);
  if(x>y)
  {
      if(x>z)
      max = x;
      else
      max = z;
  }
    else
    {
        if(y>z)
        max = y;
        else 
        max = z;
    }
    printf("Maximum value is: %d\n",max);
}
